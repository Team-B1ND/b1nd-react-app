{
  "name": "b1nd-react-app",
  "version": "1.3.1",
  "main": "generate-app",
  "scripts": {
    "clean": "rimraf dist",
    "build:main": "esbuild index.ts --bundle --platform=node --target=node18 --outfile=dist/index.js --format=cjs --banner:js=\"#!/usr/bin/env node\"",
    "copy-files": "node scripts/copy-files.js",
    "build": "pnpm run clean && pnpm run build:main && pnpm run copy-files",
    "prepublishOnly": "pnpm run build"
  },
  "type": "commonjs",
  "files": [
    "dist"
  ],
  "bin": {
    "b1nd-react-app": "./dist/index.js"
  },
  "keywords": [
    "react"
  ],
  "author": "b1nd",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Team-B1ND/b1nd-react-app.git"
  },
  "homepage": "https://github.com/Team-B1ND/b1nd-react-app",
  "description": "React BoilerPlate for DGSW Students made by B1ND",
  "devDependencies": {
    "@types/node": "20.14.2",
    "@types/prompts": "^2.4.9",
    "@vercel/ncc": "^0.38.3",
    "chalk": "^4.1.2",
    "child_process": "^1.0.2",
    "commander": "^12.1.0",
    "esbuild": "^0.25.5",
    "execa": "^9.3.1",
    "fs": "0.0.1-security",
    "inquirer": "^7.3.3",
    "nanospinner": "^1.2.2",
    "path": "^0.12.7",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "packageManager": "pnpm@9.1.1",
  "dependencies": {
    "fs-extra": "^11.3.0"
  }
}
